Program Example2; (* Factorial *)
const c = 1.2 ;
Var n : Integer;

Function F(n : FLOAT) : FLOAT;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
  For n := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI( F(n));
     Call WriteI( F(n));
    End;
End. (* Factorial *)